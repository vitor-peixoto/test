    "components"
  ],
  "author": {
    "email": "vitorpeixoto14@hotmail.com",
    "name": "vitor peixoto"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vitor-peixoto/test.git"
  },
  "bugs": {
    "url": "https://github.com/vitor-peixoto/test/issues"
  },
  "homepage": "https://github.com/vitor-peixoto/test#readme",
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript && tsc -p ./src/tsconfig.json --declaration --emitDeclarationOnly",
    "watch": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript --watch"
  },
  "dependencies": {
