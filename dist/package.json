    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@svgr/rollup": "^8.1.0",
    "@types/node": "^20.8.7",
    "@types/react": "^18.2.31",
    "rollup": "^4.1.4",
    "rollup-plugin-dts": "^6.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
